{
    "name":"asconsulting/zyppy_popup",
    "description":"A popup Module for Contao 4.x",
    "homepage": "https://andrewstevens.consulting",
    "keywords":["contao", "popup", "pop-up", "overlay"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Andrew Stevens",
            "homepage":"https://andrewstevens.consulting"
        }
    ],
    "require":{
        "php": ">=7.3",
        "contao/core-bundle": "^4.9",
        "contao-community-alliance/composer-plugin": "~2.4 || ~3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "ZyppyPopup\\": [
                "system/modules/zyppy_popup/library/ZyppyPopup"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/zyppy_popup": "system/modules/zyppy_popup"
            }
        }
    },
    "minimum-stability": "dev"
}
